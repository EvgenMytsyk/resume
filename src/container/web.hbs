{{!--
Це код container файлу для summary сторінки,
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}

<div>
    <section>
        {{#with web}}
        <div>
            <h2>Languages</h2>
            <dl> {{#each languages}}
                <b>
                    <dt>{{name}}, version: {{version}}</dt>
                </b><br>
                <dd>{{description}}</dd><br>
                <hr>

                <div>
                    <dl>
                        {{#each elements}}
                        <p><b>Elements</p></b>
                        <dt><b>{{name}}</b></dt><br>
                        <dd>{{description}}</dd><br>

                        {{#if ../attributes}}

                        <dt><b>{{name}}</b></dt><br>
                        <dd>{{description}}</dd><br>

                        {{/if}}

                        <dl>
                            <p><b>Attributes</p></b>
                            {{#each attributes}}
                            <dt><b>{{name}}</b></dt>
                            <dd>{{description}}</dd>
                            {{/each}}
                        </dl>
                        <hr>





                        {{/each}}
                    </dl>
                </div>
                <div>


                </div>







                {{/each}}
            </dl>
        </div>
        <hr>







        {{/with}}
    </section>


</div>

{{!--
Цей код містить лише те, що буде виводитись в {{{body}}} в layout/default.hbs
Адже наш layout/default.hbs має вже структуру HTML
--}}